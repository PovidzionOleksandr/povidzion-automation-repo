{
  "compilerOptions": {
    "target": "ES2019",
    "lib": ["ES2019", "DOM", "DOM.Iterable"],
    "types": [
      "cypress",
      "@testing-library/cypress",
      "@types/googletag",
      "cypress-real-events",
